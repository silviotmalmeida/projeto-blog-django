sudo docker run -it ubuntu:20.04

    apt update

    apt install python3.8 python3.8-dev python3-pip tzdata -y

    pip3 install django==4.0.1

    pip3 install pillow

    pip3 install django-summernote

    exit

sudo docker commit (id do container obtido em sudo docker ps -a) blog-django

sudo docker save blog-django > blog-django.tar
